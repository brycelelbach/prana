(load "boost.test") 

(boost:test:suite (quote phxpr:intrinsics:front) 
  ((variable l (quote (1 2 3)))
  
  ; basic
   (boost:test:=                 (front (list 1 2 3))                          3)
   (boost:test:= ((lambda (x y)  (front (list 1 x y)))                   2 3)  3) 
   (boost:test:= ((lambda (x)    (front x))                              l)    3) 

  ; nested
   (boost:test:=                 (front (front (list (list 1 2 3) 0)))         3)
   (boost:test:= ((lambda (x y)  (front (front (list (list 1 x y) 0))))  2 3)  3) 
   (boost:test:= ((lambda (x)    (front (front (list x 0))))             l)    3))) 

