(load "boost.test") 

(boost:test:suite (quote phxpr:intrinsics:eq)
  ; binary
  ((boost:test:=                   (= 1 1)                     #t)
   (boost:test:= ((lambda (x)      (= 1 x))            0)      #f)
   (boost:test:= ((lambda (x y)    (= x y))            0 0)    #t)
   (boost:test:= ((lambda (x y)    (= 1 (= x y)))      0 0)    #f)

  ; nary
   (boost:test:=                   (= 1 1 0)                   #f)
   (boost:test:= ((lambda (x)      (= 1 1 x))          1)      #t)
   (boost:test:= ((lambda (x y)    (= 0 x y))          0 1)    #f)
   (boost:test:= ((lambda (x y z)  (= 1 1 (= x y z)))  1 1 1)  #t)))

