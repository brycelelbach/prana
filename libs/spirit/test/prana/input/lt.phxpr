(load "boost/test.phxpr") 

(test-suite 'prana-unit-test-lt
  ; binary
  ((test-eq                 (< 1 0)         #f)
   (test-eq ((lambda (x)    (< 1 x))  0)    #f)
   (test-eq ((lambda (x y)  (< x y))  1 0)  #f)
   (test-eq                 (< 1 1)         #f)
   (test-eq ((lambda (x)    (< 1 x))  1)    #f)
   (test-eq ((lambda (x y)  (< x y))  1 1)  #f)
   (test-eq                 (< 2 3)         #t)
   (test-eq ((lambda (x)    (< 2 x))  3)    #t)
   (test-eq ((lambda (x y)  (< x y))  2 3)  #t)))

