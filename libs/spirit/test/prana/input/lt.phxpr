(load "boost.test") 

(boost:test:suite (quote phxpr:intrinsics:lt)
  ; binary
  ((boost:test:=                 (< 1 0)         #f)
   (boost:test:= ((lambda (x)    (< 1 x))  0)    #f)
   (boost:test:= ((lambda (x y)  (< x y))  1 0)  #f)
   (boost:test:=                 (< 1 1)         #f)
   (boost:test:= ((lambda (x)    (< 1 x))  1)    #f)
   (boost:test:= ((lambda (x y)  (< x y))  1 1)  #f)
   (boost:test:=                 (< 2 3)         #t)
   (boost:test:= ((lambda (x)    (< 2 x))  3)    #t)
   (boost:test:= ((lambda (x y)  (< x y))  2 3)  #t)))

