(load "boost/test.phxpr") 

(test-suite 'prana-unit-test-at
  ; basic
  ((define l '(1 2 3))

   (test-eq (at (list 1 2 3) 1) 2)
   (test-eq ((lambda (x) (at (list 1 2 3) x)) 2) 3)
   (test-eq ((lambda (x) (at x 1)) l) 2)
   (test-eq ((lambda (x y) (at x y)) l 2) 3)

  ; 0 index
   (test-eq ((lambda (x) (at (list 1 2 3) x)) 0) 1)
   (test-eq ((lambda (x y) (at x y)) l 0) 1)))

