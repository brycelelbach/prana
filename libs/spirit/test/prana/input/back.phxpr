(load "boost/test.phxpr") 

(test-suite 'prana-unit-test-back
  ((define l '(1 2 3))
  
  ; basic
   (test-eq                 (back (list 1 2 3))                         3)
   (test-eq ((lambda (x y)  (back (list 1 x y)))                  2 3)  3) 
   (test-eq ((lambda (x)    (back x))                             l)    3) 

  ; nested
   (test-eq                 (back (back (list 0 (list 1 2 3))))         3)
   (test-eq ((lambda (x y)  (back (back (list 0 (list 1 x y)))))  2 3)  3) 
   (test-eq ((lambda (x)    (back (back (list 0 x))))             l)    3))) 

