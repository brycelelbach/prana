(load "boost.test") 

(boost:test:suite (quote phxpr:intrinsics:not)
  ; basic
  ((boost:test:=                 (! #t)         #f)
   (boost:test:=                 (! #f)         #t)
   (boost:test:= ((lambda (x)    (! x))    #t)  #f) 
   (boost:test:= ((lambda (x)    (! x))    #f)  #t) 

  ; nested
   (boost:test:=               (! (! #t))       #t)
   (boost:test:=               (! (! #f))       #f)
   (boost:test:= ((lambda (x)  (! (! x)))  #t)  #t) 
   (boost:test:= ((lambda (x)  (! (! x)))  #f)  #f))) 

