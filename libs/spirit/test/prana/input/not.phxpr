(load "boost/test.phxpr") 

(test-suite 'prana-unit-test-not
  ; basic
  ((test-eq                 (! #t)         #f)
   (test-eq                 (! #f)         #t)
   (test-eq ((lambda (x)    (! x))    #t)  #f) 
   (test-eq ((lambda (x)    (! x))    #f)  #t) 

  ; nested
   (test-eq               (! (! #t))       #t)
   (test-eq               (! (! #f))       #f)
   (test-eq ((lambda (x)  (! (! x)))  #t)  #t) 
   (test-eq ((lambda (x)  (! (! x)))  #f)  #f))) 

