project spirit_prana_test :
  requirements
    <toolset>clang:<cxxflags>-Wno-unused
    <toolset>clang:<cxxflags>-Wno-tautological-compare
    <include>./
    <include>../../../../
    <source>/boost//unit_test_framework
  ;

run adt/symbol/size.cpp
  : : : <address-model>32 : prana_adt_symbol_size_32bit ;

run adt/symbol/deep_copy.cpp
  : : : <address-model>32 : prana_adt_symbol_deep_copy_32bit ;

run adt/sequence/size.cpp
  : : : <address-model>32 : prana_adt_sequence_size_32bit ;

run adt/sequence/forward_iteration.cpp
  : : : <address-model>32 : prana_adt_sequence_forward_iteration_32bit ;

run adt/sequence/reverse_iteration.cpp
  : : : <address-model>32 : prana_adt_sequence_reverse_iteration_32bit ;

run adt/sequence/front_removal.cpp
  : : : <address-model>32 : prana_adt_sequence_front_removal_32bit ;

run adt/sequence/back_removal.cpp
  : : : <address-model>32 : prana_adt_sequence_back_removal_32bit ;

